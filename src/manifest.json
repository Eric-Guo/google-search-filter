{
  "name": "google search filter",
  "version": "0.2",
  "description": "过滤掉 google 结果页的 链接转发，身在天朝伤不起啊",
  "manifest_version": 2,
  "content_scripts": [{
    "js": [ "filter.js" ],
    "matches": [ 
      "*://www.google.com.hk/*", 
      "*://www.google.com/*", 
      "*://www.google.cn/*", 
      "*://plus.google.com/*" 
    ],
    "run_at": "document_end"
  }],
  "homepage_url":"https://github.com/iuhoay/google-search-filter",
  "icons": { "128": "ico.png" }
}
